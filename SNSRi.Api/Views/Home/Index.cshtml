@{
    ViewBag.Title = "Rooms";
    Layout = "~/Views/Shared/_startScreen.cshtml";
}
<div class="tile-group seven" ng-controller="RoomsController">
    <span class="tile-group-title">Rooms</span>
    <div class="tile-container">
  
        <div ng-repeat="room in Rooms">
            <room-tile room="room"></room-tile>
        </div>
       
    </div>
</div>



@section scripts {
<script src="~/app/roomsController.js"></script>
}