
<div class="row cells4 action-item">
    <div class="cell">
        <span class="action-item icon mif-mail"></span>
    </div>
    <div class="cell colspan3 hover"  ng-click="editTicket(ticket)">

        <h4>{{ticket.Name}}</h4>
        <p>{{ticket.Description}}</p>
        <p>{{ticket.CreatedOn | date}}</p>
    </div>
</div>